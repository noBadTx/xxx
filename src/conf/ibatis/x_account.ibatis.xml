<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" 
	"http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="xemoo">
	<select id="query_x_account_login" parameterClass="com.xemoo.pojo.XAccount"
		resultClass="com.xemoo.pojo.XAccount">
		select * from x_account where account_type=#account_type#
		and account_login_name=#account_login_name#
	</select>
</sqlMap>